<mvc:View
    controllerName="ui5.walkthrough.controller.User"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">
  <Panel
    headerText="Welcome {userModel>/firstName} {userModel>/lastName} "
    class="sapUiResponsiveMargin"
    width="auto" >
    
    <VBox>
      <HBox>
        <Text text="User ID: " />
        <Text text="{userModel>/userId}" />
      </HBox>

      

      <HBox>
        <Text text="Email: " />
        <Text text="{userModel>/email}" />
      </HBox>

      <HBox>
        <Text text="Type: " />
        <Text text="{userModel>/type}" />
      </HBox>

      <HBox>
        <Text text="Password: " />
        <Text text="{userModel>/password}" />
      </HBox>

      <HBox>
        <Text text="Gender: " />
        <Text text="{userModel>/gender}" />
      </HBox>

      <HBox>
        <Text text="Date of Birth: " />
        <Text text="{userModel>/dob}" />
      </HBox>
    </VBox> 
    <Button
            id="helloDialogButton"
            text="My charging stations "
             icon="sap-icon://world"
            press=".onShowHello"
            class="sapUiSmallMarginEnd"/>
  </Panel>
</mvc:View>

